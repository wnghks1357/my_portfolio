  <script>
    $(function(){
      var msg = "Developer 장주환의 포트폴리오 입니다.";
      var msgArr = msg.split("");
      
      var i = 0;  
      var repeat = setInterval(function(){
        $("#headerMsg").html($("#headerMsg").html() + msgArr[i]);
        i++;
        if(i >= msgArr.length){
          clearInterval(repeat);
        }
      },200);
     
      $(".Skill_image_container").hover(function(){
        
        $(this).parents(".col-md-2").find(".service-heading").show();

      }, function(){
        $(this).parents(".col-md-2").find(".service-heading").hide();
      })
      
      
    })
  </script>

